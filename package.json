{
  "name": "pptp-action",
  "version": "2.0.0",
  "description": "GitHub Action for connecting to PPTP VPN",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint src/**/*.ts",
    "pack": "ncc build",
    "test": "jest",
    "all": "npm run build && npm run format && npm run pack && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/procare-nordic/pptp-action.git"
  },
  "keywords": [
    "actions",
    "pptp",
    "vpn"
  ],
  "author": "Kiyoshi Mizumoto",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.6.0",
    "@actions/exec": "^1.1.1",
    "tmp": "^0.2.1"
  },
  "devDependencies": {
    "@types/jest": "^27.4.1",
    "@types/node": "^18",
    "@types/tmp": "^0.2.3",
    "@typescript-eslint/parser": "^5.20.0",
    "@vercel/ncc": "^0.36.1",
    "eslint": "^8.14.0",
    "eslint-plugin-github": "^4.3.6",
    "eslint-plugin-jest": "^26.1.5",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.5.1",
    "jest-circus": "^27.5.1",
    "js-yaml": "^4.1.0",
    "prettier": "^2.6.2",
    "ts-jest": "^27.1.4",
    "typescript": "^4.6.3"
  }
}
